@model IEnumerable<WebApplication.ViewModels.Home.UserConversationsVM>

@Html.ActionLink("New conversation", "NewConversation", null, new { @class = "btn-search" })
<br/>
<br/>

<ul>
    @foreach (var conversation in Model)
    {
        <li>
            @Html.ActionLink(conversation.ConversationTitle, "Conversation", new { conversationId = conversation.ConversationId})
        </li>
    }
</ul>

